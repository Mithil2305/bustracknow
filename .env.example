# Public variables are exposed to the client via process.env.EXPO_PUBLIC_*
# Copy this file to .env and fill in your real values. Do NOT commit the real .env file.

# Firebase
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Google Maps (shared across Android/iOS/Web)
# Recommended: keep GOOGLE_MAPS_API_KEY and EXPO_PUBLIC_GOOGLE_MAPS_API_KEY the same
GOOGLE_MAPS_API_KEY=
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=

# Feature flags
EXPO_PUBLIC_ENABLE_GOD_MODE=false
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# API base URL (if using external backend)
EXPO_PUBLIC_API_URL=https://api.bustracknow.com
