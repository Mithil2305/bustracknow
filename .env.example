# Firebase Configuration (from Firebase Console > Project Settings)
EXPO_PUBLIC_FIREBASE_API_KEY=your_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:android:abcdef123456
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Maps (required for react-native-maps)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_key

# Cloudflare R2 (PDF Requirement: Zero egress fees)
EXPO_PUBLIC_CF_ACCOUNT_ID=your_cf_account_id
EXPO_PUBLIC_CF_ACCESS_KEY_ID=your_r2_access_key
EXPO_PUBLIC_CF_SECRET_ACCESS_KEY=your_r2_secret_key

# App Configuration
EXPO_PUBLIC_MIN_REDEMPTION_AMOUNT=100
EXPO_PUBLIC_POINTS_TO_INR_RATIO=100
EXPO_PUBLIC_ENABLE_GOD_MODE=false
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_PILOT_CITY=coimbatore

# Build Configuration
EXPO_PUBLIC_BUILD_VERSION=1.2.0
EXPO_PUBLIC_BUILD_NUMBER=12